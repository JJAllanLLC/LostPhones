{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.lostphones.com"
        }
      ],
      "destination": "https://lostphones.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/imei-success",
      "destination": "/imei-success.html"
    }
  ]
}

